#include <stdio.h>

int main() {
    FILE *in = fopen("input.txt", "r");
    FILE *out = fopen("output.txt", "w");
    char c, last = ' ';  
    if (!in || !out) return 1;
    while ((c = fgetc(in)) != EOF) {
        if (c != ' ' || last != ' ') {
            fputc(c, out);
        }
        last = c;
    } 
    fclose(in);
    fclose(out);
    printf("Готово!\n");
    return 0;
}
