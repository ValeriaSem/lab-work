#include <stdio.h>

void DigitCountSum(int K, int* C, int* S) {
    *C = 0;
    *S = 0;
    while (K > 0) {
        (*C)++;
        *S += K % 10;
        K /= 10;
    }
}

int main() {
    int numbers[] = {12345, 67890, 11111, 22222, 33333};  // Исправлено
    int count, sum;
    
    for (int i = 0; i < 5; i++) {
        DigitCountSum(numbers[i], &count, &sum);  // Исправлено
        printf("Для числа %d: Количество цифр = %d, сумма цифр = %d\n", numbers[i], count, sum);
    }
    
    return 0;
}
