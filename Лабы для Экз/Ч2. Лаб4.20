#include <stdio.h>

int main() {
    int m, n;
    scanf("%d %d", &m, &n);
    int a[m][n];
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    for (int j = 0; j < n; j++) {
        int min_i = 0, max_i = 0;
        for (int i = 1; i < m; i++) {
            if (a[i][j] < a[min_i][j]) min_i = i;
            if (a[i][j] > a[max_i][j]) max_i = i;
        }
        int temp = a[min_i][j];
        a[min_i][j] = a[max_i][j];
        a[max_i][j] = temp;
    }
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
