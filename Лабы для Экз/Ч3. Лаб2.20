#include <stdio.h>

double Fact2(int N) {
    double result = 1.0;
    if (N <= 0) return 1.0; 
    for (int i = N; i >= ((N%2==0)?2:1); i -= 2) {
        result *= i;
    }
    return result;
}
int main() {
    int numbers[] = {5, 6, 7, 8, 9};
    double result;    
    for (int i = 0; i < 5; i++) {
        result = Fact2(numbers[i]); 
        printf("Fact2(%d) = %.0f\n", numbers[i], result);
    }
    return 0;
}
