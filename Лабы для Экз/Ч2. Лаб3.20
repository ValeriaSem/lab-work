#include <stdio.h>

int main() {
    int n;
    printf("Введите N: ");
    scanf("%d", &n);
    int arr[n];
    printf("Введите элементы: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int max_sum = arr[0] + arr[1];
    int max_index = 0;  
    for (int i = 0; i < n-1; i++) {
        int sum = arr[i] + arr[i+1];
        if (sum > max_sum) {
            max_sum = sum;
            max_index = i;
        }
    }
    printf("%d %d\n", arr[max_index], arr[max_index+1]);
    return 0;
}
