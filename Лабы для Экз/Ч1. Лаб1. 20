#include <stdio.h>
#include <math.h>

int main() {
    double x1, y1, x2, y2;
    
    printf("Введите x1 y1 x2 y2: ");
    scanf("%lf %lf %lf %lf", &x1, &y1, &x2, &y2);
    
    double dx = x2 - x1;
    double dy = y2 - y1;
    double distance = sqrt(dx*dx + dy*dy);
    
    printf("Расстояние = %.3f\n", distance);
    
    return 0;
}
